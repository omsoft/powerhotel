<% provide(:title, "New user") %>
<h1>Create new user profile</h1>

    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= render 'form', f: f %>

      <div class="form-group row">
        <div class="col-sm-10">
          <%= f.submit "Create user", class: "btn btn-large btn-primary" %>
        </div>
      </div>
    <% end %>
